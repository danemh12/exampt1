#include<stdio.h>

int main(void)
{
int i;
float calificaciones[10],promedio=0;
   
for(i=0;i<10;i++)
    {
    printf("Dame la calificacion %d:  ",i+1);
    scanf("%f",&calificaciones[i]);
    promedio += calificaciones[i];
    printf("\n");
}
    printf("El promedio es:  %.2f\n\n",promedio/10);


}
